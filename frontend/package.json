    "test:unit": "vitest run src/components/__tests__/ src/hooks/__tests__/",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
